{
	"compilerOptions": {
		"baseUrl": "..",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ES6",
		"allowJs": true,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"lib": ["DOM", "ES5", "ES6", "ES7"],
		"skipLibCheck": true,
		"types": ["vitest/globals", "node"],
		"paths": {
			"obsidian": ["node_modules/obsidian/dist/obsidian.d.ts"]
		},
		"noEmit": true
	},
	"include": ["./**/*.ts", "../node_modules/vitest/globals.d.ts"],
	"exclude": []
}
